<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension
			point="org.eclipse.ui.popupMenus">
		<objectContribution
				adaptable="true"
				objectClass="org.eclipse.core.resources.IProject"
				nnameFilter="*"
				id="Plugin.contribution1">
		</objectContribution>
	</extension>
	<extension
			id="xmlProblem"
			name="XML Problem"
			point="org.eclipse.core.resources.markers">
		<super
				type="org.eclipse.core.resources.problemmarker">
		</super>
		<persistent
				value="true">
		</persistent>
	</extension>
	<extension
			point="org.eclipse.ui.newWizards">
		<category
				id="UDA wizard"
				name="UDA">
		</category>
		<wizard
				icon="icons/uda.gif"
				name="1. Crear nueva aplicación"
				category="UDA wizard" 
				class="com.ejie.uda.wizard.NewApplicationWizard"
				id="com.ejie.uda.wizard.NewApplicationWizard">
		</wizard>
		<wizard
				icon="icons/uda.gif"
				name="4. Añadir proyecto WAR"
				category="UDA wizard"
				class="com.ejie.uda.wizard.AddWarApplicationWizard"
				id="com.ejie.uda.wizard.AddWarApplicationWizard">
		</wizard>
		<wizard
				icon="icons/uda.gif"
				name="2. Generar código de negocio y control"
				category="UDA wizard"
				class="com.ejie.uda.wizard.GenerateCodeWizard"
				id="com.ejie.uda.wizard.GenerateCodeWizard">
		</wizard>
		<wizard
				icon="icons/uda.gif"
				name="3. Generar mantenimiento"
				category="UDA wizard"
				class="com.ejie.uda.wizard.NewMaintWizard"
				id="com.ejie.uda.wizard.NewMaintWizard">
		</wizard>
		<wizard
				icon="icons/uda.gif"
				name="7. Generar código para EJB Servidor"
				category="UDA wizard"
				class="com.ejie.uda.wizard.GenerateSkeletonWizard"
				id="com.ejie.uda.wizard.GenerateSkeletonWizard">
		</wizard>
		<wizard
				icon="icons/uda.gif"
				name="6. Generar código para EJB Cliente"
				category="UDA wizard"
				class="com.ejie.uda.wizard.GenerateStubWizard"
				id="com.ejie.uda.wizard.GenerateStubWizard">
		</wizard>
		<wizard  
				icon="icons/uda.gif"
				name="5. Añadir proyecto EJB"
				category="UDA wizard"
				class="com.ejie.uda.wizard.AddEjbApplicationWizard"
				id="com.ejie.uda.wizard.AddEjbApplicationWizard">
		</wizard>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">

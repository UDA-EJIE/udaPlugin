				<content>
					<![CDATA[
$("#${idAutocomplete}").rup_autocomplete({
	source: ${propertySource},
	sourceParam: ${propertySourceParam},
	minLength: ${propertyMinLength},
	disabled: ${propertyDisabled},
	delay: ${propertyDelay},
	defaultValue:${propertyDefaultValue}
});
